      url: '?act=newsCenter',
    }, function(res){
      var data = res.data;
      for(var i = 0; i < data.list.length; i++){
        // console.log(moment(data.list[i].publish_date).format('MMM YYYY'))
        var _html = ''
        _html += ' <div class="swiper-slide " >'
          _html += '<div class="hbox">'
            _html += '<h1>' + moment(data.list[i].year).format('YYYY/MM/DD') +'</h1>'
            _html += '<h2>' + data.list[i].title + '</h2>'
          _html += '</div>'
          _html += '<p>'
            _html += data.list[i].brief_text
          _html += '</p>'     
          _html += '<a href="/media/detail.html?id=' + data.list[i].id +' ">'
            _html += '<span class="btn_ar pointer oriMore" >了解更多'
              _html += '<img src="./assets/img/index/arr.png" class="arr" alt="">'
             _html += '</span>'
          _html += '</a>'
        _html += '</div>'
               
        $('.swiper-container1 .swiper-wrapper').append(_html)
      }
        var swiper1 = new Swiper('.swiper-container1', {
          // direction : 'horizontal',
          // pagination: '.swiper-pagination1',
          // paginationClickable: true,
          // loop: true,
          // autoplay : 1000,
          // auto: true,
          // nextButton: '.swiper-button-next',
          // prevButton: '.swiper-button-prev',
          // onlyExternal: true,
        });
        // setInterval(function(){
        //   swiper1.swipeNext()
        // }, 2000);
        $('.swiper-button-prev').on('click', function(e){
          e.preventDefault()
          swiper1.swipePrev()
        })
        $('.swiper-button-next').on('click', function(e){
          e.preventDefault()
          swiper1.swipeNext()
        })
     
    })
    var browser=navigator.appName
      var fade = true;
      var b_version=navigator.appVersion
      var version=b_version.split(";");
      var trim_Version=version.length > 1 ? version[1].replace(/[ ]/g,"") : '';
    api({
      url: '?act=kv',
    }, function(res){
      var data = res.data;
      for(var i = 0 ; i < data.length; i++){
        var _html  = ''
        if(browser=="Microsoft Internet Explorer" && trim_Version=="MSIE9.0"){
          _html = '<div class="swiper-slide"  >'
        }else{
          _html = '<div class="swiper-slide"  swiper-animate-effect="fadeInUp" swiper-animate-duration="1000ms" swiper-animate-delay="3.3s">'
        }
          _html += '<div class="kv_png" ></div>'
        _html += '</div>'
        $('.swiper-containerkv .swiper-wrapper').append(_html)
        $('.swiper-containerkv .swiper-wrapper .swiper-slide').eq(i).find('.kv_png').attr('style', "background-image: url('" + data[i].image_url + "')")

      }
      
      if(browser=="Microsoft Internet Explorer" && trim_Version=="MSIE9.0"){
          fade = false
      }
      var swiper2 = new Swiper('.swiper-containerkv', {
        // direction : 'horizontal',
        pagination: '.swiper-pagination1',
        paginationClickable: true,
        loop: fade,
        fade: fade,
        animate:fade,
        speed: 1000,

      });
    })
  })
  
</script>
