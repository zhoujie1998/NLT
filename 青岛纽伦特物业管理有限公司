  <script>
  $('.footer_end .right').on('mouseenter', function(){
      $('.wx').animate({
        opacity: 1,
      }, 300)
      setTimeout(function(){
        $('.wx').show()
      },300)
    })
    $('.footer_end .right').on('mouseleave', function(){
      $('.wx').animate({
        opacity: 0,
      }, 300)
      setTimeout(function(){
        $('.wx').hide()
      },300)
    })
</script>
